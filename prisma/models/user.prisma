model usermodel {
  id BigInt @id @default(autoincrement())
  image  userimagemodel?
  name            String
  email           String     @unique
  employeenumber  String     @unique
  phonenumber     String
  password        String
  isadmin         Boolean    @default(false)
  companyId       BigInt
  companymodel companymodel @relation(fields: [companyId], references: [id])
}
