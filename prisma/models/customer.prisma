model CustomerModel {
  id BigInt @id @default(autoincrement())
model Customer {
  id         Int        @id @default(autoincrement())
  name       String
  email      String     @unique
  phone      String?
  contracts  Contract[]
  createdAt  DateTime   @default(now())
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt 
}}
