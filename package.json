{
  "name": "nb2-dearcarmate-team1",
  "version": "1.0.0",
  "description": "**nb2-DearCarmate-team1**",
  "main": "index.js",
  "scripts": {
    "ts-node": "ts-node",
    "build": "tsc --build --clean && tsc --build",
    "start": "node dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "swagger":"swagger-cli bundle ./src/swagger/index.yaml --outfile ./src/swagger/swagger.yaml --type yaml",
    "prisma:build": "node prisma/merge-schema.js",
    "prisma:migrate": "npm run prisma:build && npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx ts-node prisma/seed.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down --volumes",
    "docker:clean": "docker compose down --volumes --rmi all --remove-orphans",
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrkgus413/nb2-dearcarmate-team1.git"
  },
  "keywords": [],
