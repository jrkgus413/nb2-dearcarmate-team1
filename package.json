  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jrkgus413/nb2-dearcarmate-team1/issues"
  },
  "homepage": "https://github.com/jrkgus413/nb2-dearcarmate-team1#readme",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/csv-parse": "^1.1.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "swagger-cli": "^4.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
